{
  "name": "vue-spa-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "dotenv -e env/.env.dev vue-cli-service serve",
    "start:test": "dotenv -e env/.env.test vue-cli-service serve",
    "build:test": "dotenv -e env/.env.test vue-cli-service build",
    "build": "dotenv -e env/.env.prod vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prepare": "husky install",
    "precommit": "eslint --fix src/**/*.{js,ts,vue}",
    "eslint": "eslint --fix src/**/*.{js,ts,vue}",
    "prettier": "prettier --write src/**/*.{js,ts,vue}",
    "stylelint": "stylelint --fix src/**/*.{vue,css,less}"
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{vue,css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^3.2.2",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.4",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^12.4.1",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "~4.1.5"
  }
}
